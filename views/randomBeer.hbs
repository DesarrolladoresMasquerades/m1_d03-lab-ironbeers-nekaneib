<div>
    <h1>Random beer:</h1>
    {{#each responseFromAPI}}
    <img src="{{image_url}}" alt="">
    <h2 class="name">{{name}}</h2>
    <p class="description">{{description}}</p>
    <p class="tagline">{{tagline}}</p>
    <p class="food pairing">{{food_pairing}}</p>
    <p class="brewer tips">{{brewers_tips}}</p>
    {{/each}}
</div>


